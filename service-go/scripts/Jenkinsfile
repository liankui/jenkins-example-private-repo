pipeline {
  agent { label '10.1.1.15' }
  tools { go '1.17.6' }

  stages {
    stage('Build') {
      steps {
        echo 'build start'
        sh 'sudo supervisorctl stop all'
        sh 'go version'
        dir('service-go/cmd') {
            sh 'go version'
            sh 'sh ./xbuild.sh'
            sh 'mv cmd /opt/abfuzz2/'
            echo 'build done'
        }
        echo 'build all done'
      }
    }

    stage('Run') {
      steps {
        echo 'done'
      }
    }
  }
}
