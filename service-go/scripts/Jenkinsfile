pipeline {
  agent any
  tools { go '1.17.6' }

  stages {
    stage('build') {
      steps {
        sh 'echo "build starting"'
        sh 'go version'
        dir('service-go/cmd') {
            sh 'go run main.go'
        }
      }
    }
    stage('execute') {
      steps {
        echo 'done'
      }
    }
  }
}
