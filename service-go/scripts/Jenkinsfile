pipeline {
  agent any
  dir('service-go') {
    echo '2'
  }
  tools { go '1.17.6' }

  stages {
    stage('build') {
      steps {
        echo 'build starting'
        sh 'go version'
        sh 'cmd/xbuild.sh'
      }
    }
    stage('execute') {
      steps {
        echo 'done'
      }
    }
  }
}
